{
    "compilerOptions": {
        "jsx": "react",
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "strictNullChecks": false,
        "target": "es5",
        "lib": ["dom", "es2015", "es2016"]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
