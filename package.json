        "rollup-plugin-uglify": "^2.0.1",
        "rollup-watch": "^4.3.1",
        "tslib": "^1.6.0",
        "typescript": "^2.5.3"
    },
    "dependencies": {
        "@types/react": "^16.0.10",
        "@types/react-dom": "^16.0.1",
        "classnames": "^2.2.5",
        "rollup-plugin-local-resolve": "^1.0.7"
    },
    "peerDependencies": {
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
