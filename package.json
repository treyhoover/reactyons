{
    "name": "reactyons",
    "version": "0.0.3",
    "description": "A library for quickly building react components with tachyons",
    "repository": {
        "type": "git",
        "url": "https://github.com/treyhoover/reactyons.git"
    },
    "author": "Trey Hoover <trey.hoover@gmail.com>",
    "license": "MIT",
    "keywords": [
        "reactyons",
        "react",
        "tachyons",
        "react-tachyons",
        "tachyons-react"
